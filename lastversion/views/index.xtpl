<!doctype html>
<html>
<head>
    <meta name="charset" content="utf-8" />
    <meta charset="gbk" />
    <meta content="text/html; charset=gbk" http-equiv="Content-Type" />
    <meta content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no,minimal-ui" name="viewport" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta content="black" name="apple-mobile-web-app-status-bar-style" />
    <meta content="telephone=no" name="format-detection" />
    <meta content="写点中文你搞得懂迈？" name="description" />
    <link rel="shortcut icon" href="/res/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon-precomposed" href="/res/icon-76@2x.png" />
    <title>{{title}}</title>
    <link rel="stylesheet" href="/css/base.css" />
    <link rel="stylesheet" href="/css/content.css" />
    <script type="text/javascript" src="http://g.tbcdn.cn/kissy/k/1.4.2/seed-min.js"></script>
</head>
<body>
    <div id="bg-container">
        <div class="container-content">
        {{#if (data.success)}}
        <header class="container-header">
            <h1 class="content-title"> finance of company </h2>
            <h2>- by ianli.</h2>
        </header>
        <h3 class="content-separate">company names</h2>
        <ul class="content-company">
        {{#each (data.names)}}
            <li{{#if (xindex===data.index)}} class="current"{{/if}}>{{this}}</li>
        {{/each}}
        </ul>
        <h3 class="content-separate">company data</h2>
        <table class="content-data">
            <thead>
                <tr>
                {{#each (data.titles)}}
                <th>{{this}}</th>
                {{/each}}
                </tr>
            </thead>
            <tbody>
                {{#each (data.data)}}
                <tr>
                    {{#each (this)}}
                    <td>{{this}}</td>
                    {{/each}}
                </tr>
                {{/each}}
            </tbody>
        </table>
        {{#else}}
        <h2>Fail...</h2>
        {{/else}}
        {{/if}}
        </div>
    </div>
    <script src="/js/init.js"></script>
</body>
</html>
